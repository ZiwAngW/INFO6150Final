{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node" 
      }
    ],
    "routes": [
      {
        "src": "/(.*)", 
        "dest": "server.js" 
      }
    ],
    "scripts": {
        "build": "npm run build",
        "start": "node index.js" 
      }
  }
  